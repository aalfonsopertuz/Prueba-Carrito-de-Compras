@{
    ViewData["Title"] = "Home Page";
}

<div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="https://m.media-amazon.com/images/I/61OnkUKFRXL._SX3000_.jpg" class="d-block w-100" alt="1 img">
        </div>
        <div class="carousel-item">
            <img src="https://images-na.ssl-images-amazon.com/images/G/01/AmazonExports/Fuji/BankPromotions/Scotiabank/CO/2022/May/3000x1200_spa._CB621594992_.jpg"
                class="d-block w-100" alt="2 img">
        </div>
        <div class="carousel-item">
            <img src="https://images-na.ssl-images-amazon.com/images/G/01/pxd/marketing/project-rio/Rio_Gateway_Hero_Desktop_3000x1200_2X-Shop_ES._CB670798599_.jpg"
                class="d-block w-100" alt="3 img">
        </div>
        <div class="carousel-item">
            <img src="https://m.media-amazon.com/images/I/61uIjme4x8L._SX3000_.jpg"
                class="d-block w-100" alt="4 img">
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>

